<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.bahmni.module.fhir2AddlExtension.api.model">

    <class name="org.bahmni.module.fhir2AddlExtension.api.model.FhirImagingStudyNote" table="imaging_study_notes">

        <id name="imagingStudyNoteId" type="int" column="imaging_study_notes_id">
            <generator class="native">
                <param name="sequence">imaging_study_notes_imaging_study_notes_id_seq</param>
            </generator>
        </id>

        <many-to-one name="imagingStudy" class="FhirImagingStudy" not-null="true" column="imaging_study_id"/>

        <many-to-one name="performer" class="org.openmrs.Provider" column="performer_id"/>

        <property name="note" type="java.lang.String" column="note"/>

        <property name="dateCreated" type="java.util.Date" column="date_created" length="19"/>

        <many-to-one name="creator" class="org.openmrs.User" not-null="true" column="creator"/>

        <many-to-one name="changedBy" class="org.openmrs.User" column="changed_by"/>

        <property name="dateChanged" type="java.util.Date" column="date_changed" length="19"/>

        <property name="voided" type="java.lang.Boolean" column="voided" length="1" not-null="true"/>

        <many-to-one name="voidedBy" class="org.openmrs.User" column="voided_by"/>

        <property name="dateVoided" type="java.util.Date" column="date_voided" length="19"/>

        <property name="voidReason" type="java.lang.String" column="void_reason" length="255"/>

        <property name="uuid" type="java.lang.String" column="uuid" length="38" unique="true"/>

    </class>

</hibernate-mapping>
